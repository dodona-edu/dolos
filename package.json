{
  "name": "dolos",
  "version": "0.0.1",
  "description": "Plagiarism detection for Dodona based on the Winnowing algorithm",
  "main": "dist/src/app.js",
  "scripts": {
    "start": "tsc && node dist/src/app.js",
    "compare": "tsc && node --no-warnings ./dist/src/command.js",
    "test-hash-performance": "tsc && time head -c 1000000 /dev/random | node dist/src/bin/hash.js > /dev/null",
    "test-winnow-performance": "tsc && time head -c 1000000 /dev/random | node dist/src/bin/hash.js > /dev/null",
    "test": "jest --coverage",
    "build": "tsc",
    "lint": "tslint 'src/**/*.ts'"
  },
  "repository": "git@github.com:dodona-edu/dolos.git",
  "author": "Bart Mesuere <Bart.Mesuere@UGent.be>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
    "jest-junit": "^7.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "nprime": "^0.2.1",
    "tree-sitter": "^0.15.8",
    "tree-sitter-c-sharp": "^0.13.0",
    "tree-sitter-haskell": "^0.13.0",
    "tree-sitter-java": "^0.13.0",
    "tree-sitter-javascript": "^0.15.0",
    "tree-sitter-python": "^0.15.0"
  }
}
