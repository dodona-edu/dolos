{
  "name": "dolos",
  "version": "0.0.1",
  "description": "Plagiarism detection for Dodona based on the Winnowing algorithm",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "make-html-file": "tsc && node ./dist/app.js -l javascript -o html samples/js/*.js > temp.html",
    "test-hash-performance": "tsc && time head -c 1000000 /dev/random | node dist/bin/hash.js > /dev/null",
    "test-winnow-performance": "tsc && time head -c 1000000 /dev/random | node dist/bin/hash.js > /dev/null",
    "test": "jest --coverage",
    "testdebug": "node inspect ./node_modules/jest/bin/jest.js --runInBand",
    "build": "tsc",
    "lint": "tslint 'src/**/*.ts'"
  },
  "repository": "git@github.com:dodona-edu/dolos.git",
  "author": "Bart Mesuere <Bart.Mesuere@UGent.be>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/node": "^12.12.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "nprime": "^0.2.1",
    "tree-sitter": "^0.15.10",
    "tree-sitter-c-sharp": "^0.13.0",
    "tree-sitter-haskell": "^0.13.0",
    "tree-sitter-java": "^0.13.0",
    "tree-sitter-javascript": "^0.15.0",
    "tree-sitter-python": "^0.15.0"
  }
}
